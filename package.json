{
  "name": "seo-trend-analyzer",
  "version": "1.0.0",
  "description": "SEO and trending keyword analysis engine for identifying high-opportunity keywords across niches",
  "type": "module",
  "bin": {
    "seo-analyzer": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "chmod +x dist/index.js",
    "watch": "tsc --watch",
    "dev": "tsx watch src/index.ts",
    "prepare": "npm run build",
    "start": "node dist/index.js"
  },
  "keywords": [
    "seo",
    "keyword-research",
    "trend-analysis",
    "redbubble",
    "marketplace-analysis"
  ],
  "author": "SEO Trend Analyzer",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "dotenv": "^16.4.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
